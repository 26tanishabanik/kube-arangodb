                    "url": "https://arangodb.github.io/kube-arangodb/docs/api/ArangoBackup.V1#specoptionstimeout-"
                  },
                  "x-kubernetes-validations": [
                    {
                      "rule": "self.spec.options.timeout == oldSelf.spec.options.timeout",
