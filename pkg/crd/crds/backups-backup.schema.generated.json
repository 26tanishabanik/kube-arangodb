              "properties": {
                "": {
                  "type": "integer",
                  "format": "int64"
                }
              },
              "externalDocs": {
                "url": "https://arangodb.github.io/kube-arangodb/docs/api/ArangoBackup.V1#speclifetime-"
              }
            },
