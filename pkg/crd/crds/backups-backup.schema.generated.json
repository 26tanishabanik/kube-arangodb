              "type": "string",
              "externalDocs": {
                "url": "https://arangodb.github.io/kube-arangodb/docs/api/ArangoBackup.V1#specpolicyname-"
              },
              "x-kubernetes-validations": [
                {
                  "rule": "self.spec.policyName == oldSelf.spec.policyName",
